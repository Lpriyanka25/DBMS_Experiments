CREATE TABLE EMPLOYEE (
    Emp_no INT PRIMARY KEY,
    E_name VARCHAR(100),
    E_address VARCHAR(255),
    E_ph_no VARCHAR(15),
    Dept_no INT,
    Dept_name VARCHAR(100),
    Job_id CHAR(10),
    Salary DECIMAL(10, 2)
);

ALTER TABLE EMPLOYEE
ADD HIREDATE DATE;

ALTER TABLE EMPLOYEE
MODIFY Job_id VARCHAR2(10);

ALTER TABLE EMPLOYEE
RENAME COLUMN Emp_no TO E_no;

ALTER TABLE EMPLOYEE
MODIFY Job_id VARCHAR2(20);
